<?xml version="1.0" encoding="utf-8"?>
<extension version="3.0" type="plugin" group="system" method="upgrade">
	<name>JTransport - System plugin</name>
	<author>vdkhai</author>
	<creationDate>January 1st, 2013</creationDate>
	<copyright>All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later.</license>
	<authorEmail>vudinhkhai@gmail.com</authorEmail>
	<version>1.0.0</version>
	<description>WebService for jtransport</description>
	<scriptfile>install.j25.php</scriptfile>
	<files>
		<folder>jtransport</folder>
		<filename plugin="jtransport">jtransport.php</filename>
	</files>
	<languages folder="language">
		<language tag="en-GB">en-GB/en-GB.plg_system_jtransport.ini</language>
		<language tag="en-GB">en-GB/en-GB.plg_system_jtransport.sys.ini</language>
	</languages>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="client_key" type="password"
					description="Key of the client side"
					label="Key of the client side"
					default="Key of the client side"
				/>
			</fieldset>
		</fields>
	</config>
</extension>