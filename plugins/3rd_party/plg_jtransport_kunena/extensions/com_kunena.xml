<?xml version="1.0" ?>
<!DOCTYPE jtransport>
<jtransport>
	<name>Kunena for Joomla! 2.5!</name>

	<!-- The folders to copy to the new site. -->
	<folders>
		<folder>administrator/components/com_kunena</folder>
		<folder>components/com_kunena</folder>
		<folder>media/kunena</folder>
	</folders>

	<!-- The tables to copy to the new site. -->
	<tables>
		<table name="fb_announcement" title="KUNENA Announcement" tbl_key="id" source="fb_announcement" destination="kunena_announcement" type="kunena" class="JTransportKunenaAnnouncement"/>

        <table name="fb_messages" title="KUNENA Message" tbl_key="id" source="fb_messages" destination="kunena_messages" type="kunena" class="JTransportKunenaMessage"/>

        <table name="fb_messages_text" title="KUNENA Message Text" tbl_key="mesid" source="fb_messages_text" destination="kunena_messages_text" type="kunena" class="JTransportKunenaMessageText"/>

		<table name="fb_attachments" title="KUNENA Attachments" tbl_key="id" source="fb_attachments" destination="kunena_attachments" type="kunena" class="JTransportKunenaAttachment"/>

		<table name="fb_categories" title="KUNENA Categories" tbl_key="id" source="fb_categories" destination="kunena_categories" type="kunena" class="JTransportKunenaCategory"/>

		<table name="fb_ranks" title="KUNENA Ranks" tbl_key="rank_id" source="fb_ranks" destination="kunena_ranks" type="kunena" class="JTransportKunenaRank"/>

		<table name="fb_sessions" title="KUNENA Sessions" tbl_key="userid" source="fb_sessions" destination="kunena_sessions" type="kunena" class="JTransportKunenaSession"/>

		<table name="fb_smileys" title="KUNENA Smileys" tbl_key="id" source="fb_smileys" destination="kunena_smileys" type="kunena" class="JTransportKunenaSmiley"/>

		<table name="fb_users" title="KUNENA Users" tbl_key="userid" source="fb_users" destination="kunena_users" type="kunena" class="JTransportKunenaUser"/>

		<table name="fb_version" title="KUNENA Version" tbl_key="id" source="fb_version" destination="kunena_version" type="kunena" class="JTransportKunenaVersion"/>
	</tables>
</jtransport>
